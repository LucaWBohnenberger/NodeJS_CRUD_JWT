<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Relat√≥rio</title>
    <style>

        table {
            width: 100%;
        }

        th{
            text-align: left;
        }


    </style>
</head>
<body>
    <div>
        <h1>Users list</h1>
        <table>
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Level</th>
                </tr>
            </thead>
            <tbody>
                <% if (users.length === 0) { %>
                    <tr>
                        <td colspan="3">No users found</td>
                    </tr>
                <% } %>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.name %></td>
                        <td><%= user.email %></td>
                        <td><%= user.level %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>